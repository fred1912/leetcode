public class Solution {
    // you need to treat n as an unsigned value
    public int hammingWeight(int n) {
        int result = 0;
        //int temp = 0;
        for (int i=0;i<32;i++,n>>=1){
            int temp = 0;
            temp = temp<<1|(n&1);
            if (temp==1) result++;
        }
        return result;
    }
}
