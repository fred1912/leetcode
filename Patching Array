public class Solution {
    public int minPatches(int[] nums, int n) {
        int i = 0;
        int count = 0;
        int add = 1;
        while (add<=n){
            if (i<nums.length&&add>=nums[i]){
                add+=nums[i];
                i++;
            }else{
                add*=2;
                count++;
            }
        }
        return count;
    }
}
